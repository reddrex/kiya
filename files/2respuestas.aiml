<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    
	<!-- Índice de respuestas del bloque Configuración y preguntas frecuentes:
	
	1. Cuestiones generales
	    - ENCENDIDO
	    - APAGADO
	    - REINICIADO
	    - ACTIVAPANTALLA
	    - DESACTIVAPANTALLA
	    - CARGAMOVIL
	    - NUEVACONTRASENA
	    - OTRACONTRASENA
	
	2. Conexiones
	    - CONEXWIFI
	    - CONEXDATOS
	    - CONEXBLUETOOTH
	
	3. Pantalla
	    - HACERCAPTURA
	    - NUEVOFONDO
	
	4. Imagen
	    - HACERFOTOVIDEO
	    - VERFOTOVIDEO
	    - BORRARFOTOVIDEO
	
	5. Sonido y tono
	    - AUMENTARVOLUMEN
	    - DISMINUIRVOLUMEN
